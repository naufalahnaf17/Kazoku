{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@app": ["src/app.tsx"],
      "@wrapper": ["src/wrapper.tsx"],
      "@utils/*": ["src/utils/*"],
      "@screens/*": ["src/screen/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["src/components/*"],
    }
  }
}
